{
  "name": "boxfish-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:fr": "ng serve --configuration=fr",
    "build": "ng build -c production",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "extract-i18n": "ng extract-i18n --output-path src/locale --format=json",
    "validate-i18n": "npm run extract-i18n && npx tsx scripts/validate-i18n.ts",
    "lint": "ng lint",
    "eslint": "npx eslint",
    "eslint:ci": "npx eslint --fix --max-warnings 0 src",
    "prettier": "npx prettier --write"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.3",
    "@angular/common": "^19.0.3",
    "@angular/compiler": "^19.0.3",
    "@angular/core": "^19.0.3",
    "@angular/forms": "^19.0.3",
    "@angular/platform-browser": "^19.0.3",
    "@angular/platform-browser-dynamic": "^19.0.3",
    "@angular/router": "^19.0.3",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@ngrx/signals": "^19.0.0",
    "@primeng/themes": "^19.0.2",
    "@tanstack/angular-query-devtools-experimental": "^5.60.5",
    "@tanstack/angular-query-experimental": "^5.60.5",
    "ansi_up": "^6.0.2",
    "date-fns": "^4.1.0",
    "pretty-bytes": "^6.1.1",
    "primeng": "^19.0.0-rc.1",
    "rxjs": "~7.8.0",
    "tailwindcss-primeui": "^0.3.4",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.4",
    "@angular/cli": "^19.0.4",
    "@angular/compiler-cli": "^19.0.3",
    "@angular/localize": "^19.0.3",
    "@testing-library/dom": "^10.4.0",
    "@types/dockerode": "^3.3.30",
    "@types/jest": "^29.5.14",
    "angular-eslint": "19.0.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.16.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.4.2",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.2",
    "typescript": "~5.6.3",
    "typescript-eslint": "8.18.0"
  }
}
